<div class="container">
  <div class="row">
    <div class="col-md-6">
      <%= form_with url: fetch_breed_path, method: :post, data: { turbo_frame: "breed_result" } do |form| %>
        <div class="form-group">
          <%= form.label :breed, 'Breed' %>
          <%= form.text_field :breed, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= form.submit 'Submit', class: 'btn btn-primary' %>
        </div>
      <% end %>
    </div>
    <div class="col-md-6">
      <%= turbo_frame_tag "breed_result" do %>
      <% end %>
    </div>
  </div>
</div>
